<%- include('../header') %>
<a class="btn btn-light" href="/categorias/nova">Nova Categoria</a><br><br>
<a class="btn btn-light" href="/publicacoes/nova">Nova Publicacao</a>
<table class="table table-hover table-striped
">
    <thead>
        <tr>
            <th scope="col">Categoria</th>
            <th scope="col">Acoes</th>
        </tr>
    </thead>
    <tbody>
        <% categorias.forEach(categoria => { %>
        <tr>        
            <td>
                <a href="/publicacoes/categoria/<%= categoria.id %> "><%= categoria.categoria %></a>
            </td>
            <td>
                <a href="/categorias/editar/<%= categoria.id %>">Editar</a>
                <a href="/categorias/excluir/<%= categoria.id %>"> Excluir</a><br/>
            </td>
            <% }) %>
        </tr>
    </tbody>
  </table>

      

<%- include('../footer') %>